{
	"expo": {
		"name": "jobuar",
		"slug": "jobuar",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/jobuar-icon.png",
		"splash": {
			"image": "./assets/jobuar-logo.png",
			"resizeMode": "contain",
			"backgroundColor": "#000000"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"androidStatusBar": {
			"backgroundColor": "#000000",
			"translucent": false
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
